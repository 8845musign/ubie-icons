{
  "name": "@ubie/ubie-icons",
  "repository": {
    "type": "git",
    "url": "https://github.com/ubie-oss/ubie-icons.git"
  },
  "version": "0.1.8",
  "description": "Icons used in Ubie products.",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js",
    "node": "./dist/esm/index.js",
    "default": "./dist/cjs/index.js"
  },
  "types": "dist/cjs/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "figma": "node ./scripts/figma.js",
    "build:svg": "svgr -d src assets",
    "build:ts": "run-p build:ts:*",
    "build:ts:esm": "tsc --project tsconfig.esm.json",
    "build:ts:cjs": "tsc --project tsconfig.cjs.json",
    "build": "yarn build:svg && yarn build:ts",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@svgr/cli": "^5.5.0",
    "@types/react": "^17.0.22",
    "@ubie/prettier-config": "^0.1.0",
    "node-fetch": "^2.6.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "react": "^17.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
